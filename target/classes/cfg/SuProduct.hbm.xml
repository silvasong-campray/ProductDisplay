<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="com.product.dto">
	<class name="SuProduct">
		<id name="id">
			<generator class="native"></generator>
		</id>
		<property name="partNumber"></property>
		<property name="productName"></property>
		<property name="productLink"></property>
		<property name="promotionPrice"></property>
		<property name="netPrice"></property>
		<property name="vendorName"></property>
		<property name="productCatagory"></property>
		<property name="productParam" length="2000"></property>
		<property name="productImage" length="1000"></property>
		<property name="storeId"></property>
		<property name="catalogId"></property>
		<property name="vendor"></property>
		<property name="createtime" type="java.lang.Long"></property>
	</class>

</hibernate-mapping>